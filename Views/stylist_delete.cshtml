@{
  Layout="Shared/_Layout.cshtml";
}


<h1>Stylists</h1>
<hr/>
<h2>Are you sure you want to delete this stylist?</h2>
<h4>@Model.GetName()</h4>
<form action="/stylist/delete/@Model.GetId()" method="post">
  <input type="hidden" name="_method" value="DELETE">
  <button type="submit">Delete</button>
</form>
<h5><a href="/">Back home</a></h5>
